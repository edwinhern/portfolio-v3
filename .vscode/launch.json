{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run in Development Mode",
			"type": "node",
			"request": "launch",
			"runtimeExecutable": "pnpm",
			"runtimeArgs": ["dev"],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"skipFiles": ["<node_internals>/**", "${workspaceFolder}/node_modules/**"]
		}
	]
}
